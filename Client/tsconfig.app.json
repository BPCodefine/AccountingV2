{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "baseUrl": "./src",
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "jszip": [
        "./node_modules/jszip/dist/jszip.min.js"
      ],
      "@env/*": ["Environments/*"]
    }
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
